{# To get the constituency Line #}
{%- macro get_constituency_line(te_name, te_party, te_state, te_constituency, member) -%}
    {{- te_name + ' ' + te_party +  ' తరఫున ' + te_state + ' లోని ' + te_constituency + ' నియోజకవర్గానికి ప్రాతినిధ్యం వహిస్తూ ' + member|string + ' [https://tewiki.iiit.ac.in/index.php?title=%E0%B0%B2%E0%B1%8B%E0%B0%95%E0%B1%8D%E2%80%8C%E0%B0%B8%E0%B0%AD లోక్ సభ] '  + v6 + ' ఉన్నారు. ' -}}
{%- endmacro -%}

{# To get the References #}
{%- macro get_references(name) -%}
    {{'\n\n=ప్రస్తావనలు=\n\n<references />'}}
{%- endmacro -%}

{# To get the Categories #}
{%- macro get_categories(categories) -%}
