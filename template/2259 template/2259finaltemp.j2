{# Variables #}
{% if te_gender == 'పురుషుడు' %}
    {% set v1 = "నాయకుడు" %}
    {% set v2 = "జన్మించాడు" %}
    {% set v3 = "ఆయన" %}
    {% set v4 = "చేసుకున్నాడు" %}
    {% set v5 = "ఇతనికి" %}
    {% set v6 = "సభ్యుడిగా" %}
    {% set v7 = "ఇతను" %}
    {% set v8 = "సభ్యుడు" %}
    {% set v9 = "ఉన్నాడు"%}
{% else %}
    {% set v1 = "నాయకురాలు" %}
    {% set v2 = "జన్మించింది" %}
    {% set v3 = "ఆమె" %}
    {% set v4 = "చేసుకుంది" %}
    {% set v5 = "ఈమెకి" %}
    {% set v6 = "సభ్యురాలిగా" %}
    {% set v7 = "ఆమె" %}
    {% set v8 = "సభ్యురాలు" %}
    {% set v9 = "ఉంది"%}
{% endif %}


{# To get the Infobox #}
{%- macro get_infobox(te_name, te_edu, te_per, te_party, te_constituency, te_state, te_nat) -%}
    {{-'{{Infobox officeholder'}} 
    {%- if te_name and te_name != 'None' -%}
        {{'\n| name = ' + te_name}}
    {%- endif -%}
    {%- if te_edu and te_edu != 'None' -%}
        {{'\n| Education = ' + te_edu}}
    {%- endif -%}
    {%- if te_per and te_per != 'None' -%}
        {{'\n| residence = ' + te_per}}
    {%- endif -%}
    {%- if te_party and te_party != 'None' -%}
        {{'\n| party = ' + te_party}}
    {%- endif -%}
    {%- if te_constituency and te_constituency != 'None' -%}
        {{'\n| constituency = ' + te_constituency}}
    {%- endif -%}
    {%- if te_state and te_state != 'None' -%}
        {{'\n| state = ' + te_state}}
    {%- endif -%}
    {%- if te_nat and te_nat != 'None' -%}
        {{'\n| citizenship = ' + te_nat}}
    {%- endif -%}
    {{'\n}}\n'}}
{%- endmacro -%}

{# To get the Intro Line #}
{%- macro get_intro_line(te_name, te_state) -%}
    {{- te_name + '<ref> '+ references +' </ref>' +' '  + te_state + ' కు ' + ' చెందిన భారతీయ '+ '[https://tewiki.iiit.ac.in/index.php?title=%E0%B0%B0%E0%B0%BE%E0%B0%9C%E0%B0%95%E0%B1%80%E0%B0%AF%E0%B0%B5%E0%B1%87%E0%B0%A4%E0%B1%8D%E0%B0%A4' + ' రాజకీయ ' +  v1 +']'+ '. ' -}}
{%- endmacro -%}

{# To get the Intro para next line #}
{%- macro get_intro_next(te_para) -%}
    {{- te_para -}}
{%- endmacro -%}

{#To get Public offices held#}
{%- macro get_offices(te_pubofc) -%}
    {%- if te_pubofc and te_pubofc  != 'None' -%}
        {{'\n=నిర్వహించిన ప్రభుత్వ కార్యాలయాలు=\n' -}}
        {{- te_pubofc  + '. '  -}}
    {%- endif -%}
{%- endmacro -%}

{#To get Previous Association with Political Parties#}
{%- macro get_association(te_prevasso) -%}
    {%- if te_prevasso and te_prevasso != 'None' -%}
        {{'\n=రాజకీయ పార్టీలతో మునుపటి అనుబంధం=\n' -}}
        {{- te_prevasso  + '. '  -}}
    {%- endif -%}
{%- endmacro -%}

{#To get Previous Membership#}
{%- macro get_membership(te_prevmem) -%}
    {%- if te_prevmem and te_prevmem != 'None' -%}
